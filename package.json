{
  "name": "aws-lambda-puppeteer-scraper",
  "version": "1.0.0",
  "description": "Simple SAM template for running a Chromium browser inside AWS Lambda",
  "main": "browser-function/app.js",
  "type": "module",
  "scripts": {
    "build": "sam build",
    "deploy": "sam deploy --guided",
    "local": "sam local invoke WikipediaScraperFunction -e event.json --debug",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "start": "node browser-function/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arunkumar201/aws-lambda-puppeteer.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "puppeteer",
    "chromium",
    "wikipedia",
    "scraping",
    "s3"
  ],
  "author": "Arun Kumar",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/credential-providers": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@sparticuz/chromium": "^123.0.1",
    "aws-sdk": "^2.1692.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.5",
    "puppeteer": "^24.10.0",
    "puppeteer-core": "^22.6.5",
    "twitter-api-v2": "^1.18.2"
  },
  "devDependencies": {
    "@types/puppeteer": "^7.0.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "prettier": "^3.2.4"
  }
}

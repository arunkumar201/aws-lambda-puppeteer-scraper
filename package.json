{
  "name": "aws-lambda-puppeteer-scraper",
  "version": "1.0.0",
  "description": "Scalable TypeScript backend for scraper with AWS Lambda,SQS and S3 support",
  "author": {
    "name": "Arun Kumar",
    "url": "https://github.com/arunkumar201"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arunkumar201/aws-lambda-puppeteer.git"
  },
  "bugs": {
    "url": "https://github.com/arunkumar201/aws-lambda-puppeteer/issues"
  },
  "homepage": "https://github.com/arunkumar201/aws-lambda-puppeteer#readme",
  "keywords": [
    "aws",
    "lambda",
    "puppeteer",
    "scraper",
    "typescript",
    "sqs",
    "s3",
    "serverless",
    "web-scraping",
    "chromium"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "rm -rf dist/ node_modules .aws-sam/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "build": "tsc --noEmit false --strict true --forceConsistentCasingInFileNames true --skipLibCheck true",
    "lambda:build": "chmod +x ./deploy.sh && ./deploy.sh",
    "lambda:deploy": "sam deploy",
    "lambda:deploy:guided": "sam deploy --guided",
    "sqs:monitor": "npx tsx scripts/pollSqs.ts",
    "local:api": "sudo DOCKER_HOST=unix://$HOME/.docker/run/docker.sock sam local start-api -t template.local.yaml --skip-pull-image --warm-containers LAZY --port 3000 --docker-network host",
    "local:invoke": "sudo DOCKER_HOST=unix://$HOME/.docker/run/docker.sock sam local invoke -t template.local.yaml -e event.json --docker-network host",
    "dev": "nodemon src/index.ts",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-sqs": "^3.825.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@sparticuz/chromium": "^133.0.0",
    "aws-lambda": "^1.0.7",
    "cheerio": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-serve-static-core": "^0.1.1",
    "helmet": "^7.1.0",
    "http-status": "^2.1.0",
    "proxy-chain": "^2.5.9",
    "puppeteer-core": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "serverless-http": "^3.2.0",
    "turndown": "^7.2.0",
    "user-agents": "^1.1.564",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/turndown": "^5.0.5",
    "rimraf": "^6.0.1",
    "@types/aws-lambda": "^8.10.149",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-serve-static-core": "^4.17.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@types/user-agents": "^1.0.4",
    "@types/uuid": "^9.0.8",
    "esbuild": "^0.25.5",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.0",
    "puppeteer": "^24.10.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
